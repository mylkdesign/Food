        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".slider-wrapper": {
              "justify-content": "space-between",
              "width": "75%",
              "height": "100%",
              "margin": "0 auto",
              "text-align": "center"
            },
            ".title-block": {
              "padding-bottom": "2.5rem"
            },
            ".text-wrapper__text": {
              "line-height": "40px",
              "color": "#999999"
            },
            ".text-wrapper": {
              "height": "60%",
              "align-items": "center"
            },
            ".author-review": {
              "height": "35%",
              "align-items": "center",
              ".img": {
                "width": "80px",
                "height": "80px",
                "amp-img": {
                  "border-radius": "100%"
                }
              }
            },
            ".author-review__title": {
              "padding-top": "1.875rem"
            },
            ".author-review__title-name": {
              "color": "#000"
            },
            ".author-review__title-work": {
              "color": "#fabb47"
            },
            "@colorArrowVar": "replace(~\"@{colorArrow}\", \"#\", \"\")",
            ".amp-carousel-button-next": {
              "background-image": "url(\"data:image/svg+xml;charset=utf-8,%3Csvg xmlns='http://www.w3.org/2000/svg' width='24' height='24' fill='%23@{colorArrowVar}' viewBox='0 0 24 24'%3E%3Cpath d='M4.6 22.6L15.2 12 4.6 1.4 6 0l12 12L6 24z'/%3E%3C/svg%3E\")",
              "right": "15px"
            },
            ".amp-carousel-button-prev": {
              "background-image": "url(\"data:image/svg+xml;charset=utf-8,%3Csvg xmlns='http://www.w3.org/2000/svg' width='24' height='24' fill='%23@{colorArrowVar}' viewBox='0 0 24 24'%3E%3Cpath d='M19.4 1.4L8.8 12l10.6 10.6L18 24 6 12 18 0z'/%3E%3C/svg%3E\")",
              "left": "15px"
            },
            ".amp-carousel-button": {
              "background-color": "@colorBgArrow",
              "@media (min-width: 992px)": {
                "&:hover": {
                  "background-color": "@colorBgHoverArrow"
                }
              }
            },
            ".dots span when (@showDots)": {
              "background-color": "#999999",
              "&.current": {
                "background-color": "#3e976b"
              }
            },
            "@media (max-width: 767px)": {
              ".amp-carousel-button": {
                "display": "none"
              },
              ".dots-wrapper": {
                "padding-top": "10px"
              }
            },
            "H3": {
              "color": "#fabb47"
            }
          },
